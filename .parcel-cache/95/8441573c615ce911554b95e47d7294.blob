@-ms-viewport {
  width: device-width;
}

body {
  font-family: "Nunito Sans", sans-serif;
  color: white;
}

button {
  font-family: "Nunito Sans", sans-serif;
  color: white;
}

button:hover{
    background: #0053ba;
}

button:focus {
    outline: 1px solid #fff;
    outline-offset: -4px;
}

button:active {
    transform: scale(0.99);
}

h1,
h2,
h3,
h4,
h5,
h6 {
  font-family: "Maven Pro", sans-serif;
}
